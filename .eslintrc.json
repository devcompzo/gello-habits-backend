{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eqeqeq": "error",
        "no-console": "warn",
        "linebreak-style": [
            "error"
        ]
    },
    "ignorePatterns": [
        "node_modules"
    ]
}